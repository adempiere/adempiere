    ELEMENT
    -->
    <!-- applet -->
	<xs:element name="applet" type="appletType" />
	<xs:complexType name="appletType">
		<xs:group ref="baseGroup" minOccurs="0" maxOccurs="unbounded" />
		<xs:attributeGroup ref="xulElementAttrGroup" />
